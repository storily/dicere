{
    "name": "storily/dicere",
    "license": "Apache-2.0",
    "type": "project",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.9",
        "fideloper/proxy": "^3.3.4",
        "folklore/graphql": "^1.0.25",
        "itsdamien/laravel-heroku-config-parser": "^1.1",
        "laravel/lumen-framework": "^5.5.0",
        "mashape/unirest-php": "^3.0.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7.1",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.3.0",
        "vlucas/phpdotenv": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "minimum-stability": "dev",
    "optimize-autoloader": true,
    "prefer-stable": true
}
